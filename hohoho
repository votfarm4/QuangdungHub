-- MAIN TAB
local MainTab = Window:CreateTab("üè† Main", 4483362458)

-- AUTO FARM SECTION
local AutoFarmSection = MainTab:CreateSection("Auto Farm")

AutoFarmSection:CreateToggle({
    Name = "Auto Farm Level",
    CurrentValue = false,
    Flag = "AutoFarmLevel",
    Callback = function(Value)
        getgenv().AutoFarm = Value
        
        if Value then
            spawn(function()
                while wait() and getgenv().AutoFarm do
                    pcall(function()
                        -- Find nearest enemy
                        local nearest = nil
                        local dist = math.huge
                        
                        for i, v in pairs(Workspace.Enemies:GetChildren()) do
                            if v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                local mag = (LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
                                if mag < dist then
                                    dist = mag
                                    nearest = v
                                end
                            end
                        end
                        
                        if nearest then
                            -- Equip best weapon
                            local maxDmg = 0
                            local bestWeapon = ""
                            
                            for i, v in pairs(LocalPlayer.Backpack:GetChildren()) do
                                if v:IsA("Tool") and v:FindFirstChild("Damage") then
                                    if v.Damage.Value > maxDmg then
                                        maxDmg = v.Damage.Value
                                        bestWeapon = v.Name
                                    end
                                end
                            end
                            
                            if bestWeapon ~= "" then
                                LocalPlayer.Character.Humanoid:EquipTool(LocalPlayer.Backpack[bestWeapon])
                            end
                            
                            -- Teleport to enemy
                            LocalPlayer.Character.HumanoidRootPart.CFrame = nearest.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0)
                            
                            -- Attack
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AttackEntity", nearest.Name)
                            
                            -- Auto click
                            VirtualUser:CaptureController()
                            VirtualUser:ClickButton1(Vector2.new())
                        end
                    end)
                end
            end)
        end
    end,
})

AutoFarmSection:CreateToggle({
    Name = "Auto Farm Boss",
    CurrentValue = false,
    Flag = "AutoFarmBoss",
    Callback = function(Value)
        getgenv().AutoBoss = Value
        
        if Value then
            spawn(function()
                while wait() and getgenv().AutoBoss do
                    pcall(function()
                        local bosses = {
                            "Saber Expert",
                            "Darkbeard",
                            "Dough King",
                            "Cake Queen",
                            "Diamond",
                            "Jeremy",
                            "Fajita",
                            "Smoke Admiral",
                            "Cursed Captain"
                        }
                        
                        for _, bossName in pairs(bosses) do
                            for i, v in pairs(Workspace:GetChildren()) do
                                if v.Name == bossName and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                    repeat wait(0.5)
                                        LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 25, 0)
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AttackEntity", v.Name)
                                    until not getgenv().AutoBoss or v.Humanoid.Health <= 0
                                end
                            end
                        end
                    end)
                end
            end)
        end
    end,
})

