-- RAID TAB
local RaidTab = Window:CreateTab("‚öîÔ∏è Raid", 4483362458)

local RaidSection = RaidTab:CreateSection("Raid Features")

RaidSection:CreateToggle({
    Name = "Auto Raid",
    CurrentValue = false,
    Flag = "AutoRaid",
    Callback = function(Value)
        getgenv().AutoRaid = Value
        
        if Value then
            spawn(function()
                while wait() and getgenv().AutoRaid do
                    pcall(function()
                        -- Check if can raid
                        if LocalPlayer.Data.Fragments.Value >= 1000000 then
                            -- Start raid
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("RaidsNpc", "Select", getgenv().SelectedRaid or "Flame")
                            
                            -- Teleport to raid
                            local raidCF = CFrame.new(-12476.6182, 374.999237, -7551.21826)
                            LocalPlayer.Character.HumanoidRootPart.CFrame = raidCF
                            
                            -- Auto kill raid mobs
                            for i, v in pairs(Workspace.Enemies:GetChildren()) do
                                if v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                    if string.find(v.Name, "Raid") then
                                        LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0)
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AttackEntity", v.Name)
                                    end
                                end
                            end
                        end
                    end)
                end
            end)
        end
    end,
})

