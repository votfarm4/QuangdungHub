    Options = {"Flame", "Ice", "Quake", "Light", "Dark", "String", "Rumble", "Magma", "Human: Buddha", "Sand", "Bird: Phoenix"},
    CurrentOption = "Flame",
    Flag = "RaidSelection",
    Callback = function(Option)
        getgenv().SelectedRaid = Option
    end,
})

